{
  "name": "csik",
  "version": "1.0.0",
  "description": "Client Script Injection Kit",
  "main": "server.js",
  "repository": "git@github.com:mgillam/csik.git",
  "author": "Mic <mic@secureideas.com>",
  "license": "GPL-3.0",
  "type": "module",
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "eslint": "^6.0.0"
  },
  "scripts": {
    "start": "node --experimental-modules server.js",
    "build": "rollup --config --format iife"
  },
  "eslintConfig": {
    "env": {
      "es6": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2017,
      "allowImportExportEverywhere": true
    },
    "rules": {
      "no-var": "error"
    }
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "immutable": "^4.0.0-rc.12",
    "lodash": "^4.17.11",
    "rollup": "^1.16.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-javascript-obfuscator": "^1.0.4",
    "rollup-plugin-multi-input": "^1.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "socket.io": "^2.2.0"
  }
}
